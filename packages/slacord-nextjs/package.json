{
    "name": "@slacord/nextjs",
    "version": "1.0.0",
    "description": "Slacord Next.js web dashboard",
    "private": true,
    "scripts": {
        "dev": "next dev --port 3002",
        "build": "next build",
        "start": "next start --port 3002",
        "lint": "next lint",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\" --ignore-path .gitignore",
        "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\" --ignore-path .gitignore",
        "clean": "rm -rf .next node_modules"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.2.4",
        "@tailwindcss/postcss": "^4.1.18",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "framer-motion": "^12.23.26",
        "lucide-react": "^0.562.0",
        "next": "^16.1.1",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "@types/react": "^19.2.7",
        "autoprefixer": "^10.4.23",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.1",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3"
    }
}
